{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": [".", "../../../../README.md"],
		"includePattern": ".+\\.js(doc|x)?$",
		"exclude": ["node_modules"],
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": false,
		"showInheritedInNav": true,
		"default": {
			"outputSourceFiles": false,
			"includeDate": false
		}
	},
	"opts": {
		"destination": "../../../../docs/potential_disorders_detection",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/jsdoc-skyceil"
	}
}